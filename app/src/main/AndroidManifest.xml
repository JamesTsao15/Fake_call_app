<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fake_call2">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fake_call2">
        <activity android:name=".dark_theme_activity"></activity>
        <activity android:name=".Answer_Call_Activity" />
        <activity
            android:name=".Record_Audio_Activity"
            android:label="Record Audio" />
        <activity
            android:name=".Choose_Caller_Activity"
            android:exported="false"
            android:label="Choose Caller" />
        <activity
            android:name=".Full_color_caller_theme_Activity"
            android:exported="false" />
        <activity
            android:name=".Theme_Activity"
            android:exported="false"
            android:label="Select Theme" />
        <activity
            android:name=".Schedule_Call_Activity"
            android:label="Schedule Call" />
        <activity
            android:name=".Caller_Activity"
            android:label="Caller" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>